{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier",
    "react",
    "jsx-a11y",
    "import",
    "jest"
  ],
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "prettier/prettier": ["error", {
      "trailingComma": "es5",
      "semi": true,
      "singleQuote": true,
      "printWidth": 100
    }]
  },
  "env": {
    "jasmine": true,
    "jest": true
  }
}
